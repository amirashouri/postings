package views

import (
    "main/db/sqlc"
)

templ Home(items []db.Post, loggedIn bool) {
    <div class="flex place-content-center py-4">
        <div class="grid grid-cols-1 flex-1 mb-6">
        <div class="posts">
            for _, item := range items {
            @Post(item)
        }
        </div>
        @CreatePost("", "", "")
        </div>
    </div>
}